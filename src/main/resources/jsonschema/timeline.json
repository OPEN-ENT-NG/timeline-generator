{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string"
        },
        "headline": {
            "type": "string"
        },
        "text": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "icon": {
            "type": "string"
        },
        "created": {
            "type": "object"
        }, 
        "modified": {
            "type": "object"
        },
        "owner": {
            "type": "object"
        },
        "trashed": {
            "type": "boolean"
        },
        "folder": {
            "type": "number"
        }
